<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/cell_fragment">

    <fragment
        android:id="@+id/cell_fragment"
        android:name="com.example.ui.CellsFragment"
        android:label="cell_fragment"
        tools:layout="@layout/cell_fragment" >
        <action
            android:id="@+id/action_cellFragment_to_numberFragment"
            app:destination="@id/numberFragment" />
    </fragment>
    <fragment
        android:id="@+id/numberFragment"
        android:name="com.example.ui.NumberFragment"
        android:label="fragment_number"
        tools:layout="@layout/fragment_number" >
        <action
            android:id="@+id/action_numberFragment_to_cellFragment"
            app:destination="@id/cell_fragment" />
    </fragment>
</navigation>